{
  "name": "darkindy/spring-password-utils",
  "description": "PHP utilities for encoding and validating passwords in the same way as Spring Security's StandardPasswordEncoder in Java.",
  "keywords": [
    "java",
    "spring",
    "password",
    "utilities"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrei Pietrusel",
      "email": "andreipietrusel@gmail.com"
    }
  ],
  "type": "project",
  "require": {
    "php": ">=5.3.3",
    "symfony/polyfill-mbstring": "1.9"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.35"
  },
  "autoload": {
    "psr-4": {
      "Darkindy\\SpringPasswordUtils\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "phpunit --verbose --configuration phpunit.xml"
    ]
  }
}
